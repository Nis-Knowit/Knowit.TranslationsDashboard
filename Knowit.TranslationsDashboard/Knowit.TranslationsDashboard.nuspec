<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Knowit.TranslationsDashboard</id>
    <version>1.0.0</version>
    <title>Knowit Translations Dashboard</title>
    <authors>Knowit</authors>
    <description>A translation management dashboard for Umbraco that allows administrators to manage dictionary items in a user-friendly interface with advanced features like hierarchy view, pagination, and search.</description>
    <projectUrl>https://github.com/knowit/umbraco-translations-dashboard</projectUrl>
    <repository type="git" url="https://github.com/knowit/umbraco-translations-dashboard" />
    <icon>icon.png</icon>
    <readme>README.md</readme>
    <license type="expression">MIT</license>
    <tags>umbraco umbraco-marketplace translations dictionary localization i18n</tags>
    <dependencies>
      <group targetFramework="net8.0">
        <dependency id="Umbraco.Cms.Web.Website" version="13.0.0" />
        <dependency id="Umbraco.Cms.Web.BackOffice" version="13.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="icon.png" target="\" />
    <file src="README.md" target="\" />
    <file src="build\**\*.*" target="build" />
    <file src="App_Plugins\**\*.*" target="content\App_Plugins" />
  </files>
</package>